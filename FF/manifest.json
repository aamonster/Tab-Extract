{
  "manifest_version": 2,
  "name": "Tab Extract",
  "version": "1.0.2",
  "description": "A simple & lightweight tool to reduce tab clutter, using just the address bar.",  
  "icons": {
    "48": "icons/icon_048.png"
  },
  "omnibox": {"keyword": "ex"},
  "permissions": ["tabs"],
  "background": {
    "scripts": ["tabExtract.js"]
  },  
  "applications": {
    "gecko": {
      "strict_min_version": "52.0a1"
    }
  }  
}